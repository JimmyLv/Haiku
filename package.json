  "name": "Haiku",
  "version": "1.0.0",
  "description": "A powerful rich interactive web application by using nobackend tech stack.",
  "main": "./src/index.jsx",
  "scripts": {
    "clean": "rm -rf public && rm -rf _site",
    "dev": "webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --colors --hide-modules",
    "lint": "eslint src/. --ext .jsx --ext .js; exit 0",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "fix": "eslint --fix src/. --ext .jsx --ext .js; exit 0",
    "test": "karma start --single-run=true",
    "cdn": "cd scripts && ./qrsync-v2 conf.json",
    "prod": "npm run fix && npm run test && npm run build && npm run cdn",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "start": "npm run clean && npm run build && jekyll serve"
  },
